{
	"name": "clone-tabnews-2",
	"version": "1.0.0",
	"description": "Exerc√≠cio do Curso.Dev do Caro Filipe Deschamps",
	"main": "index.js",
	"scripts": {
		"dev": "npm run services:up && next dev",
		"lint:check": "prettier --check .",
		"lint:fix": "prettier --write .",
		"test": "cross-env-shell NODE_ENV=${NODE_ENV:-development}  jest",
		"test:watch": "npm run test -- --watchAll --runInBand",
		"services:up": "docker compose -f infra/compose.yaml up -d --force-recreate",
		"services:stop": "docker compose -f infra/compose.yaml stop",
		"services:down": "docker compose -f infra/compose.yaml down",
		"migrate:create": "node-pg-migrate -m infra/migrations create",
		"migrate:up": "node-pg-migrate -m infra/migrations --env-path .env.development up"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"dotenv": "^16.4.4",
		"next": "^13.1.6",
		"node-pg-migrate": "^6.2.2",
		"pg": "^8.11.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"jest": "^29.6.2",
		"prettier": "^3.4.2"
	},
	"imports": {
		"#infra*": "./infra*"
	}
}
